  "redirect-rules": [
    {
      "src": "^https\\:\\/\\/github.com\\/[^/]*/[^/]*\\/releases\\/download\\/.*",
      "kind": "regexp",
      "dst": "https://github.uon.workers.dev/$0",
      "enable": true
    },
    {
      "src": "^https:\\/\\/github\\.com\\/(.*)\\/archive/(.*)\\.zip",
      "kind": "regexp",
      "dst": "https://github.uon.workers.dev/https://github.com/$1/archive/$2.zip",
      "enable": true
    },
    {
      "src": "^https:\\/\\/github\\.com\\/(.*)\\/archive\\/(.*)\\.tar\\.gz",
      "kind": "regexp",
      "dst": "https://github.uon.workers.dev/https://github.com/$1/archive/$2.tar.gz",
      "enable": true
    },
    {
      "src": "www.google.com/recaptcha",
      "kind": "wildcard",
      "dst": "recaptcha.net/recaptcha",
      "enable": true
    }
  ],
  "cancel-rules": [],
  "request-headers": [],
  "response-headers": []
}